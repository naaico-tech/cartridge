# Cartridge Backend Environment Variables
# Copy this file to .env and update with your actual API keys

# AI Provider API Keys
OPENAI_API_KEY=your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here
GEMINI_API_KEY=your-gemini-api-key-here

# Database URLs
CARTRIDGE_DB_URL=postgresql://cartridge:cartridge@localhost:5432/cartridge
CARTRIDGE_REDIS_URL=redis://localhost:6379/0

# External URLs (for localhost access)
CARTRIDGE_DB_URL_EXTERNAL=postgresql://cartridge:cartridge@localhost:5432/cartridge
CARTRIDGE_REDIS_URL_EXTERNAL=redis://localhost:6379/0